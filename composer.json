{
    "name": "gzhegow/lang",
    "description": "Lang Module",
    "type": "library",
    "authors": [
        {
            "name": "gzhegow",
            "email": "6562680@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/6562680/support"
        },
        {
            "type": "vcs",
